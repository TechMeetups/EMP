<% if @event_user.nil? %>
	$(".token-input-delete-token-facebook").trigger("click")
	alertify.alert("Selected Users Already added.")
<% else %>	
	$(".token-input-delete-token-facebook").trigger("click")
	<% if !@attendee.nil? %>
		$("#attendee_block").css("margin-top", "5px")

		$("#attendee_list").html("<%= escape_javascript(render 'events/attendees_list') %>")
	<% elsif !@speakers.nil? %>
		$("#speaker_block").css("margin-top", "5px")

		$("#speaker_list").html("<%= escape_javascript(render 'events/speakers_list') %>")
	<% elsif !@partners.nil? %>
		$("#partner_block").css("margin-top", "5px")

		$("#partner_list").html("<%= escape_javascript(render 'events/partners_list') %>")
	<% end %>
<% end %>



