<div id="import_event"  style="background: none repeat scroll 0px 0px lightgray;">
	<% debugger %>
	<%  @results_events.each_with_index do |import_event, index|  %>
		<% if  import_event.created_at ==  import_event.updated_at%>
			<ul class="a">
				<li>	
				<lable style="margin-left:30px;font-size: 15px;font-weight:bold;"><%= import_event.title %>.......added.</lable>
				</li>
			</ul>
		<% else %>
		<ul class="a">
				<li>	
				<lable style="margin-left:30px;font-size: 15px;font-weight:bold;"><%= import_event.title %>.......updated.</lable>
				</li>
			</ul>
		<% end %>
		<%  @results_attendees.each_with_index do |import_attendee, index|  %>
			<% if import_attendee.created_at == import_attendee.updated_at %>
				<ul class="a">
					<li>	
						<lable style="margin-left:70px;font-size: 15px;"><%= index+1 %>. <%= import_attendee.name %>.......added.</lable>
					</li>
				</ul>
			<% else %>
			<ul class="a">
					<li>	
						<lable style="margin-left:70px;font-size: 15px;"><%= index+1 %>. <%= import_attendee.name %>.......updated.</lable>
					</li>
				</ul>
			<% end %>
		<% end %>
		<% @results_attendees=[] %>
	<% end %>
</div>

<button type="button" class="btn btn-default" data-dismiss="modal" style="margin-left:320px;margin-bottom:20px;">Done</button>

<style>
.a{
list-style-type: none;
}
</style>