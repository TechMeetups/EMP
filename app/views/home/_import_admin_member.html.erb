<div id="import_member" style="background: none repeat scroll 0px 0px lightgray;margin-left: 22px;width: 550px;">
	<%if params[:city_id]=="http://www.meetup.com/TechMeetups-Berlin" %>
		<h4 style="margin-left: 20px; font-weight: bold;padding-top: 25px;">Berlin:  Berlin Group</h4>
	<% elsif params[:city_id]=="http://www.meetup.com/london-silicon-roundabout/" %>
		<h4 style="margin-left: 10px; font-weight: bold;padding-top: 25px;">London:  London Silicon Roundabout Group</h4>
	<% else %>
		<h4 style="margin-left: 10px; font-weight: bold;padding-top: 25px;">New York:  New York Silicon Alley Group</h4>
	<% end %>
	<br />
	
	<% @user_exist.each_with_index do |import_member, index|  %>
		<ul class="a">
			<li>
				<lable style="margin-left:50px;font-size: 15px;"><%= index+1 %>.<%= import_member  %>............added</lable>
			</li>
		</ul>
	<% end %>
</div>
<button type="button" class="btn btn-default" data-dismiss="modal" style="margin-left:250px;margin-bottom:20px;margin-top:10px;">Done</button>
<style>
.a{
list-style-type: none;
}
</style>
