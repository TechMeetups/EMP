<div class="partner">
  <%= image_tag "missing.png", :style=>"height: 63px; width:73px;" %>
</div>
<div Style="height: 34px; width:73px; ">
<%= form_for @add_event_user, :url=> add_more_path, :remote=>true do |f| %>
	<%= f.hidden_field :event_id, :value=>@event.id %>

	<%= f.text_field :user_id, :id=>"event_user_partner_tokens" %>

	<%= f.hidden_field :event_type, :id=>"dynamic_partner",:value =>"Partner" %>
	<%= image_submit_tag("plus.png", :class=>"plus_partners") %>
  <%= link_to(image_tag("cancel1.png", :class=>"cancel_partners"), event_path) %>
<% end %>
</div>
<script>
  $(function() {
    $("#event_user_partner_tokens").tokenInput("/events/<%= params[:id]%>.json", {
      crossDomain: false,
      prePopulate: $("#event_user_partner_tokens").data("pre"),
      theme: "facebook",
      tokenLimit :1,
      preventDuplicates: true
    });
  });
</script>