<div id="event_address">
	<b> Address </b> 
	<br/><br/><%= @event.event_users.where(event_type: "Venue").first.user.try(:address) if !@event.event_users.where(event_type: "Venue").first.nil? %><br/><br/>
	<b> City </b> <%=  @event.event_users.where(event_type: "Venue").first.user.try(:city).try(:name) if !@event.event_users.where(event_type: "Venue").first.nil? %> <br/><br/>
	<iframe width="200" height="200" frameborder="0" scrolling="no" marginheight="0" marginwidth="0" src="https://maps.google.co.in/maps?client=ubuntu&amp;channel=cs&amp;ie=UTF8&amp;q=<%= @event.event_users.where(event_type: "Venue").first.user.try(:address) if !@event.event_users.where(event_type: "Venue").first.nil?  %>&amp;fb=1&amp;gl=in&amp;hq=&amp;iwloc=A&amp;output=embed"></iframe>
</div>