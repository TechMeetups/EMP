<div Style="height: 34px; width:73px; ">
<%= form_for @add_event_user, :url=> add_more_path, :remote=>true do |f| %>
	<%= f.hidden_field :event_id, :value=>@event.id %>

	<%= f.text_field :user_id , :id=>"event_user_venue_tokens"%>
	<%= f.hidden_field :event_type, :id=>"dynamic_venue" %>
	<%= image_submit_tag("plus.png", :style=>"margin: 5px -12px -7px 85px" )%>
<% end %>
</div>
<script>
  $(function() {
    $("#event_user_venue_tokens").tokenInput("/events/<%= params[:id]%>.json", {
      crossDomain: false,
      prePopulate: $("#event_user_venue_tokens").data("pre"),
      theme: "facebook",
      placeholder: 'Place holder text...',
      tokenLimit :1,
      preventDuplicates: true

    });
  });
</script>